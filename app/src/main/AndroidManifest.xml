<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.belapp">

    <application
        android:name=".GlobalGame"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BelApp">
        <activity android:name=".PointsNormal"></activity>
        <activity android:name=".roundHistory" />
        <activity android:name=".HistoryOfPartijas" />
        <activity android:name=".Points" />
        <activity android:name=".Settings" />
        <activity android:name=".History" />
        <activity android:name=".NormalGame" />
        <activity android:name=".FastGame" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>